# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET           /empresas             controllers.EmpresaController.findAll
GET           /empresas/:id         controllers.EmpresaController.find(id: Integer)
POST          /empresas             controllers.EmpresaController.save
PATCH         /empresas             controllers.EmpresaController.update
DELETE        /empresas/:id         controllers.EmpresaController.delete(id: Integer)
GET           /proyectos            controllers.ProyectoController.findAll
GET           /proyectos/:id        controllers.ProyectoController.find(id: Integer)
POST          /proyectos            controllers.ProyectoController.save
PATCH         /proyectos            controllers.ProyectoController.update
DELETE        /proyectos/:id        controllers.ProyectoController.delete(id: Integer)
